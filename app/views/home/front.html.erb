<div class="row">
  <div id="newsfeed" class="col-lg-12">
    <h4> <%= link_to "Inicia session", new_user_session_path %></h4>
    <div id="activities">
      <% if @activities.empty? %>
        <div class="well">
          <%= render('shared/no_resource', resources: 'Activities') %>
        </div>
      <% else %>
        <%= render_activities(@activities, layout: '/shared/activity') %>
      <% end %>
    </div>
    <%= render 'shared/paginate', resources: @activities %>
  </div>
</div>
